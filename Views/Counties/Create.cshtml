@model ASE_Election_Portal_G20.Models.County

@{
    ViewData["Title"] = "Add County";
}

<div>
    <a asp-action="Index">Back to List</a>
    <h2 style="text-align: center">Add County</h2>
</div>
<hr />
<div class="row">
    <div class="center-align">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group d-flex">
                <label asp-for="CountyName" class="col-md-5 control-label required"></label>
            <div>
                <input asp-for="CountyName" class="form-control" />
                <span asp-validation-for="CountyName" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group d-flex">
                <label asp-for="StateId" class="col-md-5 control-label required"></label>
                <select asp-for="StateId" class ="form-control" asp-items="ViewBag.StateId"></select>
            </div>
          
            <div style="text-align: center;margin-top:20px">
                <input type="submit" value="Add County" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


